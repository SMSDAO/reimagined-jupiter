{
  "name": "gxq-studio",
  "version": "1.0.0",
  "description": "The most advanced Solana flash loan arbitrage system",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "start:railway": "node dist/src/index-railway.js",
    "dev": "ts-node-esm src/index.ts",
    "test": "jest",
    "test:farcaster": "npm run build && node dist/scripts/testFarcaster.js",
    "lint": "eslint src --ext .ts",
    "setup-env": "bash scripts/setup-env.sh",
    "deploy:vercel": "vercel --prod",
    "deploy:railway": "bash scripts/migrate-to-railway.sh",
    "cli": "ts-node-esm cli/index.ts",
    "admin": "ts-node-esm cli/commands/admin.ts"
  },
  "keywords": [
    "solana",
    "flash-loan",
    "arbitrage",
    "defi",
    "jupiter",
    "quicknode"
  ],
  "author": "GXQ STUDIO",
  "license": "MIT",
  "dependencies": {
    "@jup-ag/api": "^6.0.0",
    "@pythnetwork/client": "^2.22.1",
    "@pythnetwork/hermes-client": "^2.0.0",
    "@solana/spl-token": "^0.3.9",
    "@solana/web3.js": "^1.87.6",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@types/pg": "^8.15.6",
    "@vercel/node": "^3.0.12",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "bs58": "^5.0.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "inquirer": "^9.2.12",
    "jsonwebtoken": "^9.0.2",
    "ora": "^7.0.1",
    "pg": "^8.16.3"
  },
"scripts": {
  "build": "tsc && tsc-alias",
  "pre-deploy": "node scripts/pre-deploy-check. ts",
  "deploy": "npm run pre-deploy && vercel --prod",
  "deploy:staging": "vercel",
  "test": "jest --coverage",
  "test:integration": "jest --testMatch '**/*. integration.test.ts'",
  "logs": "vercel logs",
  "monitor": "node scripts/monitor-health.ts"
}
